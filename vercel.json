{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/main.js"
    }
  ],
  "includeFiles": ["dist/**/*", "node_modules/**/*", ".env", ".env.local"],
  "env": {
    "NODE_ENV": "production"
  }
}
